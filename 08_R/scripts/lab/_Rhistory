basics<- function(x){
c(min(x),mean(x),max(x))
}
vapply(temp,basics,numeric(1))
# Usando vapply aplica la funciÃƒÂ³n basics
vapply(temp,basics,numeric(3))
basics <- function(x) {
c(min = min(x), mean = mean(x), median = median(x), max = max(x))
}
vapply(temp, basics, numeric(3))
# Arregla el error:
vapply(temp, basics, numeric(4))
