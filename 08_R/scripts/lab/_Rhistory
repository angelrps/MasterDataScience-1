vapply(temp, function(x, y) { mean(x) > y }, y = 5, logical(1))
get_info <- function(x, y) {
if (mean(x) > y) {
return("Not too cold!")
} else {
return("Pretty cold!")
}
}
sapply(temp, get_info, y = 5)
# Convierte este sapply en un vapply
vapply(temp, get_info, y = 5, character(13))
# Convierte este sapply en un vapply
vapply(temp, get_info, y = 5, character(1))
